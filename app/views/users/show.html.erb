<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @user.name %>
</p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<p>
  <strong>Profile pic:</strong>
  <%= image_tag(@user.profile_pic) %>
</p>

<% @user.pictures.each do |picture| %>
  <%= image_tag(picture.url) %>
<% end %>

<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
